[tool.black]
line-length = 120
target-version = ['py310']
include = '\.pyi?$'
exclude = '''
      /(
        | \.git
        | \.__pycache__
        | \.hg
        | \.mypy_cache
        | \.pre-commit-config.yaml
        | \.proto
        | \.tox
        | \.venv
        | venv
        |  _build
        |  buck-out
        |  build
        |  dist
      )/
      '''
